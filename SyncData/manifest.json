{
    "name": "Sync Data To ( application name)",
    "description": "description of my extension",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "./popup.html",
        "default_icon": {
            "16": "images/tick.png",
            "48": "images/tick.png",
            "128": "images/tick.png"
        }
    },
    "background": {
        "service_worker": "./backgroundPage.js"
    },
    "permissions": [
        "scripting",
        "tabs",
        "activeTab"
    ],
    "host_permissions": [
        "https://datatables.net/extensions/searchbuilder/examples/initialisation/html.html"
    ],
    "icons": {
        "16": "images/tick.png",
        "48": "images/tick.png",
        "128": "images/tick.png"
    }
}